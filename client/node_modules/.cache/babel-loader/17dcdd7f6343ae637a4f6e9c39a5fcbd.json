A,IAAA,EAAA;AADA,OAAA;AAGA,UAAA,KAAA,GAAA,MAAA,MAAA,CAAA,IAAA,GAAA,MAAA,CAAA,KAAA;AACA,UAAA,MAAA,GAAA,MAAA,MAAA,CAAA,GAAA,GAAA,MAAA,CAAA,MAAA;AAEA,UAAA,GAAA,GAAA,EAAA,CAAA,MAAA,CAAA,KAAA,EACA,IADA,CACA,OADA,EACA,KAAA,GAAA,MAAA,CAAA,IAAA,GAAA,MAAA,CAAA,KADA,EAEA,IAFA,CAEA,QAFA,EAEA,MAAA,GAAA,MAAA,CAAA,GAAA,GAAA,MAAA,CAAA,MAFA,CAAA;AAGA,WAAA,SAAA,GAAA,GAAA,CACA,MADA,CACA,GADA,EAEA,IAFA,CAEA,WAFA,sBAEA,MAAA,CAAA,IAFA,cAEA,MAAA,CAAA,GAFA,OAAA;AAIA,UAAA,CAAA,GAAA,EAAA;AAEA,UAAA,MAAA,GAAA,EAAA,CAAA,WAAA,GACA,MADA,CACA,CAAA,CAAA,EAAA,CAAA,GAAA,CAAA,CADA,EAEA,KAFA,CAEA,CAAA,CAAA,EAAA,KAAA,CAFA,CAAA;AAIA,UAAA,MAAA,GAAA,EAAA,CAAA,WAAA,GACA,MADA,CACA,CAAA,CAAA,EAAA,CAAA,CADA,EAEA,KAFA,CAEA,CAAA,MAAA,EAAA,CAAA,CAFA,CAAA;AAIA,WAAA,SAAA,CAAA,MAAA,CAAA,GAAA,EACA,IADA,CACA,OADA,EACA,QADA,EAEA,IAFA,CAEA,WAFA,yBAEA,MAFA,QAGA,IAHA,CAGA,EAAA,CAAA,UAAA,CAAA,MAAA,CAHA;AAKA,WAAA,SAAA,CAAA,MAAA,CAAA,GAAA,EACA,IADA,CACA,OADA,EACA,QADA,EAEA,IAFA,CAEA,EAAA,CAAA,QAAA,CAAA,MAAA,CAFA;AAGA;AA9EA,GAxCA;AAwHA,EAAA,OAxHA,qBAwHA;AACA,SAAA,OAAA;AACA,SAAA,OAAA;AACA,SAAA,YAAA;AACA;AA5HA,CAAA","sourcesContent":["<template>\n<div class=\"row\">\n      <div class=\"col-md-7 border border-dark rounded\" id=\"column_one\">\n        <div class=\"row\" id=\"first_row\">\n          <div class=\"col-md-6\">\n            <button type=\"button\" class=\"btn btn-dark text\"\n            @click=\"getButtonName('CMAQ',$event), getdata($event)\"> CMAQ </button>\n          </div>\n          <div class=\"col-md-6\">\n            <button type=\"button\" class=\"btn btn-dark text\"\n            @click=\"getButtonName('Our Method',$event), getdata($event)\">\n            Our Method </button>\n          </div>\n        </div>\n        <hr>\n        <div class=\"row\" id=\"third_row\">\n          <div class=\"col-md-6 seperate\">\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <label class=\"labels text\">Future Hour:</label>\n              </div>\n              <div class=\"col-md-6\">\n                <select name=\"Please select a pollutant\"\n                  @change= \"getdata($event)\"\n                  class=\"filter_option\" v-model=\"hour\">\n                  <option v-for=\"Future_hour in Future_hours\" v-bind:key=\"Future_hour.hour\">\n                        {{ Future_hour.hour }}\n                      </option>\n                </select>\n              </div>\n            </div>\n          </div>\n          <div class=\"col-md-6\">\n            <button type=\"button\" class=\"btn btn-dark text\"\n            @click=\"parameters.animate = !parameters.animate, getdata($event)\"> Animate </button>\n          </div>\n        </div>\n        <hr>\n        <div class=\"row\" id=\"fourth_row\">\n          <div class=\"col-md-6 seperate\">\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <label class=\"labels text\">Station:</label>\n              </div>\n              <div class=\"col-md-6\">\n                <input class=\"user_input form-control\"\n                @keyup.enter = \"getdata()\" v-model=\"parameters.Station_code\"\n                id=\"Station_Code\" placeholder=\"Enter Station\">\n              </div>\n            </div>\n          </div>\n          <div class=\"col-md-6\">\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <label class=\"labels text\">On-site:</label>\n              </div>\n              <div class=\"col-md-6\">\n                <input class=\"user_input form-control\"\n                @keyup.enter = \"getdata()\" v-model=\"parameters.Onsite_code\"\n                id=\"Onsite_Code\" placeholder=\"Enter On-site\">\n              </div>\n            </div>\n          </div>\n        </div>\n        <hr>\n        <div class=\"row\" id=\"fifth_row\">\n          <div class=\"col-lg-6 seperate\" id='lineChart1'>\n            <svg></svg>\n          </div>\n          <div class=\"col-lg-6\" id='lineChart2'>\n          </div>\n        </div>\n        <hr>\n        <div class=\"row\" id=\"sixth_row\">\n          <div class=\"col-md-6 seperate\">\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <label class=\"labels text\">IOA:</label>\n              </div>\n              <div class=\"col-md-6\">\n                <p>IOA</p>\n              </div>\n            </div>\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <label class=\"labels text\">RMSE:</label>\n              </div>\n              <div class=\"col-md-6\">\n                <p>RMSE</p>\n              </div>\n            </div>\n          </div>\n          <div class=\"col-md-6\">\n            <div class=\"row\">\n              <p>IOA</p>\n            </div>\n            <div class=\"row\">\n              <p>RMSE</p>\n            </div>\n          </div>\n        </div>\n    </div>\n      <div class=\"col-md-5 border border-dark rounded\">\n        <div class=\"row\" id=\"first_row_1\">\n          <div class=\"col-md-6\">\n           <p>Date Picker</p>\n          </div>\n          <div class=\"col-md-6\">\n            <div id=\"filter\">\n              <label id=\"label_6\" class='filter_value text'>Pollutant:</label>\n              <select name=\"Please select a pollutant\"\n              @change= \"getdata($event)\"\n                    class=\"filter_option\" v-model=\"parameters.selected\">\n                <option v-for=\"Pollutant_name in Pollutant_names\" v-bind:key=\"Pollutant_name.name\">\n                        {{ Pollutant_name.name }}\n                      </option>\n              </select>\n            </div>\n          </div>\n        </div>\n        <h2>{{ result_data }} Result</h2>\n        <div id=\"map\">\n          Here goes the map\n        </div>\n      </div>\n    </div>\n</template>\n<script>\nimport * as d3 from 'd3';\nimport 'leaflet/dist/leaflet.css';\nimport L from 'leaflet';\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      hour: 0,\n      Pollutant_names: [\n        { name: 'NO2' },\n        { name: 'O3' },\n        { name: 'PM10' },\n        { name: 'PM25' },\n        { name: 'SO2' },\n      ],\n      Future_hours: [\n        { hour: '0' },\n        { hour: '1' },\n        { hour: '2' },\n        { hour: '3' },\n        { hour: '4' },\n        { hour: '5' },\n        { hour: '6' },\n        { hour: '7' },\n        { hour: '8' },\n        { hour: '9' },\n        { hour: '10' },\n        { hour: '11' },\n        { hour: '12' },\n      ],\n      parameters: [\n        { Station_code: 'AA' },\n        { Onsite_code: 'AB' },\n        { selected: 'PM25' },\n        { result_data: 'CMAQ' },\n        { animate: false },\n      ],\n      map: null,\n      lineChart: null,\n      tileLayer: null,\n      geodata: [],\n      polygon_data: [],\n    };\n  },\n  methods: {\n    getButtonName(name) {\n      this.parameters.result_data = name;\n    },\n    getdata() {\n      this.clearLayer();\n      let polygon;\n      const colors = d3.scaleQuantize()\n        .domain([0, 30])\n        .range(['#c4e6c3', '#96d2a4', '#6dbc90', '#4da284', '#36877a', '#266b6e', '#1d4f60']);\n      const path = 'http://127.0.0.1:5000/data';\n      axios.post(path, {\n        // pollutants: this.parameters.selected,\n        // Result_data: this.parameters.result_data,\n        // Animate: this.parameters.animate,\n        // Future_hour: this.parameters.hour,\n        // Station_code: this.parameters.Station_code,\n        // Onsite_code: this.parameters.Onsite_code,\n        parameters\n      })\n        .then((res) => {\n          this.geodata = res.data;\n          this.geodata.forEach((layer) => {\n            const val = layer.pollutant;\n            polygon = L.polygon(layer.coord, {\n              fillColor: colors(layer.pollutant), color: 'grey', weight: 1, opacity: 0, fillOpacity: 0.5,\n            }).bindTooltip(val.toString());\n            polygon.addTo(this.map);\n            this.polygon_data.push(polygon);\n          });\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n    clearLayer() {\n      this.polygon_data.forEach((layer) => {\n        this.map.removeLayer(layer);\n      });\n    },\n    initMap() {\n      this.map = L.map('map').setView([22.34385, 114.1289], 10);\n      this.tileLayer = L.tileLayer('https://cartodb-basemaps-{s}.global.ssl.fastly.net/rastertiles/voyager/{z}/{x}/{y}.png',\n        { attribution: '&copy; <a href=\"http://www.openstreetmap.org/copyright\">OpenStreetMap</a>, &copy; <a href=\"https://carto.com/attribution\">CARTO</a>' });\n      this.tileLayer.addTo(this.map);\n    },\n    renderChart1() {\n      const margin = {\n        top: 30, right: 50, bottom: 30, left: 30,\n      };\n      const width = 450 - margin.left - margin.right;\n      const height = 450 - margin.top - margin.bottom;\n\n      const svg = d3.select('svg')\n        .attr('width', width + margin.left + margin.right)\n        .attr('height', height + margin.top + margin.bottom);\n      this.lineChart = svg\n        .append('g')\n        .attr('transform', `translate(${margin.left},${margin.top})`);\n\n      const n = 21;\n\n      const xScale = d3.scaleLinear()\n        .domain([0, n - 1])\n        .range([0, width]);\n\n      const yScale = d3.scaleLinear()\n        .domain([0, 1])\n        .range([height, 0]);\n\n      this.lineChart.append('g')\n        .attr('class', 'x axis')\n        .attr('transform', `translate(0, ${height})`)\n        .call(d3.axisBottom(xScale));\n\n      this.lineChart.append('g')\n        .attr('class', 'y axis')\n        .call(d3.axisLeft(yScale));\n    },\n  },\n  mounted() {\n    this.initMap();\n    this.getdata();\n    this.renderChart1();\n  },\n};\n</script>\n<style>\n/* Column 1 */\n/* ids */\n#first_row, #second_row, #third_row, #fourth_row, #fifth_row, #sixth_row{\n  padding: 2% 0 1% 2%;\n  text-align: left;\n}\n#lineChart1, #lineChart2 {\n  height: 450px;\n  text-align: center;\n}\n/* tags */\nhr{\n  margin: 0;\n  width: 102%;\n}\np{\n  text-align: center;\n  margin: 2%;\n}\n/* class */\n.btn{\n  margin: 0 25%;\n  width: 130px;\n}\n.labels{\n  margin-top: 5%;\n}\n.seperate{\n  border-right: 1px solid lightgrey;\n}\n.text{\n  font-weight: bold;\n  font-size: 17px;\n}\n/* Column 2 */\n/* ids */\n#map {\n  text-align: center;\n  height: 450px;\n  width: 650px;\n  margin: 10% 3%;\n}\n#filter {\n  padding-top: 5%;\n  padding-left: 40%;\n}\n/* tag */\nh2 {\n  margin : 15% 0 0 0;\n}\n</style>\n"],"sourceRoot":"src/components"}]}